<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="405dp"
        android:layout_height="587dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView108"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/baner_detalles4" />

        <ImageButton
            android:id="@+id/reglas_cerrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="360dp"
            android:background="@drawable/efecto_cerrar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView108"
            app:srcCompat="@drawable/cerrar1" />

        <Spinner
            android:id="@+id/apuesta_sp1"
            android:layout_width="132dp"
            android:layout_height="28dp"
            android:layout_marginTop="216dp"
            android:background="#FAFAFA"
            app:layout_constraintEnd_toEndOf="@+id/imageView108"
            app:layout_constraintHorizontal_bias="0.747"
            app:layout_constraintStart_toStartOf="@+id/imageView108"
            app:layout_constraintTop_toTopOf="@+id/imageView108" />

        <ImageButton
            android:id="@+id/imageButton70"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="184dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.546"
            app:layout_constraintStart_toStartOf="@+id/imageView108"
            app:layout_constraintTop_toTopOf="@+id/imageView108"
            app:srcCompat="@android:drawable/star_big_on" />

        <ImageButton
            android:id="@+id/imageButton71"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="184dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.632"
            app:layout_constraintStart_toStartOf="@+id/imageView108"
            app:layout_constraintTop_toTopOf="@+id/imageView108"
            app:srcCompat="@android:drawable/star_big_on" />

        <ImageButton
            android:id="@+id/imageButton72"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="184dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.718"
            app:layout_constraintStart_toStartOf="@+id/imageView108"
            app:layout_constraintTop_toTopOf="@+id/imageView108"
            app:srcCompat="@android:drawable/star_big_on" />

        <ImageButton
            android:id="@+id/imageButton73"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="184dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.804"
            app:layout_constraintStart_toStartOf="@+id/imageView108"
            app:layout_constraintTop_toTopOf="@+id/imageView108"
            app:srcCompat="@android:drawable/star_big_on" />

        <TextView
            android:id="@+id/textView44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="116dp"
            android:text="APUESTAS  DE ESTRELLAS"
            android:textColor="#030304"
            android:textSize="14sp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView108"
            app:layout_constraintHorizontal_bias="0.813"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView108" />

        <ImageButton
            android:id="@+id/apuesta_b1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/imageView108"
            app:layout_constraintHorizontal_bias="0.703"
            app:layout_constraintStart_toStartOf="@+id/imageView108"
            app:layout_constraintTop_toBottomOf="@+id/apuesta_sp1"
            app:srcCompat="@drawable/boton_registro" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>