        <ImageButton
            android:id="@+id/imageButton19"
            android:layout_width="64dp"
            android:layout_height="67dp"
            android:layout_marginTop="196dp"
            android:background="@drawable/efecto_button_mariposa"
            android:onClick="estadisticas"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.12"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/botonmariposa" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="68dp"
            android:text="Estadisticas"
            android:textAlignment="center"
            android:textColor="#F8F4F4"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageButton19"
            app:layout_constraintHorizontal_bias="0.875"
            app:layout_constraintStart_toStartOf="@+id/imageButton19"
            app:layout_constraintTop_toTopOf="@+id/imageButton19" />

        <ImageButton
            android:id="@+id/Eventtesla_b2"
            android:layout_width="57dp"
            android:layout_height="64dp"
            android:layout_marginTop="200dp"
            android:background="@drawable/efecto_button_evento"
            android:onClick="salonEventos2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.189"
            app:layout_constraintStart_toEndOf="@+id/imageButton19"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/boton_evento" />

        <TextView
            android:id="@+id/Eventtesla_l1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="64dp"
            android:text="Eventos"
            android:textAlignment="center"
            android:textColor="#F6EEEE"
            app:layout_constraintEnd_toEndOf="@+id/Eventtesla_b2"
            app:layout_constraintStart_toStartOf="@+id/Eventtesla_b2"
            app:layout_constraintTop_toTopOf="@+id/Eventtesla_b2" />

        <ImageButton
            android:id="@+id/imageButton25"
            android:layout_width="65dp"
            android:layout_height="60dp"
