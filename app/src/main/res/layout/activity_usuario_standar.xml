            android:layout_width="136dp"
            android:layout_height="139dp"
            android:layout_marginTop="44dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.403"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <RadioButton
                android:id="@+id/dimenInicial_1Gb"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:text="@string/ayuda_6"
                android:textColor="#F1EEEE" />

            <RadioButton
                android:id="@+id/dimenInicial_2Gb"
                android:layout_width="match_parent"
                android:layout_height="31dp"
                android:text="@string/ayuda_7"
                android:textColor="#EAE4E4" />

            <RadioButton
                android:id="@+id/dimenInicial_3Gb"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:text="@string/ayuda_8"
                android:textColor="#FAF9F9" />

            <RadioButton
                android:id="@+id/dimenInicial_4Gb"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:text="@string/ayuda_9"
                android:textColor="#F4F2F2" />
        </RadioGroup>

        <ImageButton
            android:id="@+id/perfilB1"
            android:layout_width="98dp"
            android:layout_height="30dp"
            android:layout_marginTop="76dp"
            android:onClick="accionElegirDimencio"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.56"
            app:layout_constraintStart_toEndOf="@+id/EleccionDimencionGb"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/boton_registro" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:text="Clasificatoria"
            android:textColor="#F4F3F3"
