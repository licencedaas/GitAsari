<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo1"
    tools:context=".confi2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout47"
        android:layout_width="417dp"
        android:layout_height="73dp"
        android:background="@drawable/fondo_logo"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView145"
            android:layout_width="115dp"
            android:layout_height="55dp"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.983"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/slogan" />

        <ImageButton
            android:id="@+id/imageButton82"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:background="@drawable/efecto_cerrar"
            android:onClick="cerrar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.086"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/cerrar1" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout56"
        android:layout_width="355dp"
        android:layout_height="589dp"
        android:layout_marginTop="72dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView59"
            android:layout_width="212dp"
            android:layout_height="22dp"
            android:layout_marginTop="40dp"
            android:text="CONFIGURACION  DUNGEON"
            android:textColor="#EFE9E9"
            android:textStyle="normal|bold"
            app:layout_constraintBottom_toTopOf="@+id/imageView123"
            app:layout_constraintEnd_toEndOf="@+id/imageView123"
            app:layout_constraintHorizontal_bias="0.545"
            app:layout_constraintStart_toStartOf="@+id/imageView123"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView123"
            android:layout_width="345dp"
            android:layout_height="68dp"
            android:layout_marginTop="112dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/confi" />

        <ImageView
            android:id="@+id/imageView129"
            android:layout_width="345dp"
            android:layout_height="68dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView123"
            app:srcCompat="@drawable/confi" />

        <RadioButton
            android:id="@+id/r1_cofig1"
            android:layout_width="212dp"
            android:layout_height="45dp"
            android:layout_marginTop="20dp"
            android:text="EVENTO"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView123"
            app:layout_constraintHorizontal_bias="0.796"
            app:layout_constraintStart_toStartOf="@+id/imageView123"
            app:layout_constraintTop_toTopOf="@+id/imageView123" />

        <RadioButton
            android:id="@+id/r2_cofig"
            android:layout_width="212dp"
            android:layout_height="45dp"
            android:layout_marginTop="88dp"
            android:text="EVENTO"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView123"
            app:layout_constraintHorizontal_bias="0.796"
            app:layout_constraintStart_toStartOf="@+id/imageView123"
            app:layout_constraintTop_toTopOf="@+id/imageView123" />

        <ImageButton
            android:id="@+id/config_delet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:background="#00FFFFFF"
            android:onClick="deletcalabozo"
            app:layout_constraintEnd_toEndOf="@+id/imageView123"
            app:layout_constraintHorizontal_bias="0.025"
            app:layout_constraintStart_toStartOf="@+id/imageView123"
            app:layout_constraintTop_toTopOf="@+id/textView59"
            app:srcCompat="@android:drawable/ic_menu_delete" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>