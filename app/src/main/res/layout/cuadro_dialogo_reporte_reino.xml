<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="405dp"
        android:layout_height="587dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView64"
            android:layout_width="389dp"
            android:layout_height="378dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.497"
            app:srcCompat="@drawable/cortina1" />

        <ImageButton
            android:id="@+id/reglas_cerrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="304dp"
            android:background="@drawable/efecto_cerrar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.142"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView64"
            app:srcCompat="@drawable/cerrar1" />

        <TextView
            android:id="@+id/codigo_reporte_l4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Buen dia duelista "
            android:textColor="#F8F4F4"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.715"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/codigo_reporte_l3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:text="Aqui esta el codigo de su rival"
            android:textColor="#F8F4F4"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.817"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/codigo_reporte_l2"
            android:layout_width="104dp"
            android:layout_height="73dp"
            android:layout_marginTop="64dp"
            android:text="Reporte de victoria"
            android:textColor="#F8F4F4"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.143"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/codigo_reporte_l1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="92dp"
            android:text="CODIGO"
            android:textColor="#FF9800"
            android:textSize="48sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.775"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <ImageButton
            android:id="@+id/reporte_b1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="168dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.737"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64"
            app:srcCompat="@drawable/boton_registro" />

        <EditText
            android:id="@+id/reporte_t1"
            android:layout_width="109dp"
            android:layout_height="35dp"
            android:layout_marginTop="148dp"
            android:ems="10"
            android:hint="Digite codigo"
            android:inputType="textPassword"
            android:textColor="#E91E63"
            android:textColorHighlight="#FAF6F6"
            android:textColorHint="#FAF4F0F0"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.128"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <ImageButton
            android:id="@+id/reporte_b2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="64dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.651"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64"
            app:srcCompat="@android:drawable/arrow_down_float" />

        <ImageView
            android:id="@+id/final_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/pegasusfinal" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>