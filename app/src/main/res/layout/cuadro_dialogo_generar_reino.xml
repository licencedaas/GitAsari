<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="405dp"
        android:layout_height="587dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView64"
            android:layout_width="389dp"
            android:layout_height="378dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.497"
            app:srcCompat="@drawable/cortina1" />

        <ImageButton
            android:id="@+id/reglas_cerrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="304dp"
            android:background="@drawable/efecto_cerrar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.142"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView64"
            app:srcCompat="@drawable/cerrar1" />

        <TextView
            android:id="@+id/codigo_reporte_l4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Genere codigo si perdio el duelo"
            android:textColor="#F8F4F4"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.856"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/codigo_reporte_l2"
            android:layout_width="107dp"
            android:layout_height="63dp"
            android:layout_marginTop="64dp"
            android:text="Comparta el codigo a su rival"
            android:textColor="#F8F4F4"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.126"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <ImageButton
            android:id="@+id/reporte_b1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="168dp"
            android:background="#00FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.737"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64"
            app:srcCompat="@drawable/boton_generar" />

        <TextView
            android:id="@+id/generar_l1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:text="CODIGO"
            android:textColor="#FFC107"
            android:textSize="48sp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.793"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/textView46"
            android:layout_width="109dp"
            android:layout_height="40dp"
            android:layout_marginTop="128dp"
            android:text="Estrellas Apostadas :"
            android:textColor="#F44336"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.128"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <TextView
            android:id="@+id/genere_l2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="180dp"
            android:text="0"
            android:textColor="#F6F5F5"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.309"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64" />

        <ImageView
            android:id="@+id/imageView111"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="180dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView64"
            app:layout_constraintHorizontal_bias="0.182"
            app:layout_constraintStart_toStartOf="@+id/imageView64"
            app:layout_constraintTop_toTopOf="@+id/imageView64"
            app:srcCompat="@android:drawable/star_big_on" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>