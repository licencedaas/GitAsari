            android:textColor="#F6F2F2"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageButton45"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/imageButton45"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/imageButton65"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="144dp"
            android:background="@drawable/efecto_button_entrada_reino"
            android:onClick="eventoReinoDuelos"
